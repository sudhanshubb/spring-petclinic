pipeline {
   agent any
    tools{
        maven 'maven'
        jdk 'jdk8'
    }
   
stages {
      
stage('Checkout') {
         steps {
            checkout([$class 'GitSCM', branches [[name 'master']], doGenerateSubmoduleConfigurations false, extensions [], submoduleCfg [], userRemoteConfigs [[url 'httpsgithub.comdeepakgoel22spring-petclinic.git']]])
         }
      }
      
stage('Build'){
          steps{
              bat 'mvn clean package'
          }
      }
      
stage('Static code analysis - Sonar scanner'){
          steps{
              bat 'mvn sonarsonar'
          }
      }
   }
}

